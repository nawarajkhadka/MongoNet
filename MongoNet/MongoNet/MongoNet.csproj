<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Authors>Nawaraj Khadka</Authors>
    <Description>A generic repo for crud and search operation with mongodb in .net</Description>
    <PackageTags>dotnet;package;mongonet;mongodb;dotnet;mongodbwithdotnet;csharpmongodb</PackageTags>
    <PackageProjectUrl>https://github.com/nawarajkhadka/MongoNet</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MongoDB.Bson" Version="3.3.0" />
    <PackageReference Include="MongoDB.Driver" Version="3.3.0" />
  </ItemGroup>

</Project>
